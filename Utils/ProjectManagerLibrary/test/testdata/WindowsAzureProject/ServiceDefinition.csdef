    </Imports>
    <Endpoints>
      <InputEndpoint localPort="8093" name="Debugging1" port="8093" protocol="tcp"/>
      <InputEndpoint localPort="7070" name="DbgEp" port="7070" protocol="tcp"/>
    </Endpoints>
  </WorkerRole>
</ServiceDefinition>
